{
  "name": "wykop-plus-plus",
  "version": "1.1.17",
  "description": "Rozszerzenie zapewniające dodatkowe funkcjonalności dla portalu Wykop.pl",
  "scripts": {
    "build-dev": "webpack --config webpack/webpack.config.dev.js --colors",
    "build-dev:watch": "webpack --watch --config webpack/webpack.config.dev.js --colors",
    "build-prod": "webpack --config webpack/webpack.config.prod.js --colors",
    "build-prod:watch": "webpack --watch --config webpack/webpack.config.prod.js --colors",
    "firefox:watch": "web-ext run --source-dir ./build/ --firefox=firefoxdeveloperedition --start-url=wykop.pl",
    "lint": "tslint --project tsconfig.json src/**/**.ts",
    "test": "ls",
    "preversion": "npm test && npm run lint",
    "version": "node scripts/version-upgrade.js && npm run build-prod -- --versionUpgrade && git add -A"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Humberd/Wykop-plus-plus.git"
  },
  "keywords": [
    "chrome-extension",
    "firefox-addon",
    "typescript"
  ],
  "author": "Maciej Sawicki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Humberd/Wykop-plus-plus/issues"
  },
  "devDependencies": {
    "@types/chrome": "0.0.76",
    "@types/node": "^10.12.18",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^4.0.0-beta.2",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.2",
    "tslint": "^5.12.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.2.2",
    "web-ext": "^2.9.3",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4",
    "yargs": "^12.0.5"
  }
}
